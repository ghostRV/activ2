<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.deevrv.activ2">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">




        <activity
            android:name=".ThreeThreeActivity"
            android:parentActivityName=".ThreeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ThreeActivity">

            </meta-data>
        </activity>
        <activity
            android:name=".twoOneActivity"
            android:parentActivityName=".TwoActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TwoActivity" />
        </activity>
        <activity
            android:name=".MainActivity2"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="anrdoid.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TwoActivity" />
        <activity android:name=".ThreeActivity" />
    </application>

</manifest>